{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateConfig": {
    "Limiters": [
      {
        "Name": "Token1",
        "Type": "TokenRate",
        "PermitLimit": 1,
        "QueueLimit": 1,
        "ReplenishPeriod": "0.00:00:00.500",
        "TokensReplenishedPerPeriod": 1,
        "AutoReplenish": true
      }
    ],
    "PartitionResources": [
      {
        "LimiterName": "Token1",
        "SharePartitionLimiter": false,
        "Resource" :  "User"
      }
    ]
  },
  "AllowedHosts": "*"
}
