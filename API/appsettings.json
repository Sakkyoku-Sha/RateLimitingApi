{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateConfig": {
    "Limiters": [
      {
        "Name": "Token1",
        "Type": "TokenRate",
        "PermitLimit": 10,
        "QueueLimit": 1,
        "ReplenishPeriod": "0.00:01:00.000",
        "TokensReplenishedPerPeriod": 1,
        "AutoReplenish": true
      }
    ],
    "Partitions": [
      {
        "Name":  "UserToken",
        "LimiterName": "Token1",
        "SharePartitionLimiter": false,
        "Resource": "User"
      }
    ]
  },
  "AllowedHosts": "*"
}
